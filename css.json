{
	"css": {
		"prefix": "basecss",
		"body": [
			"@charset \"UTF-8\";",
			"",
			"html {",
			"	width: 100%",
			"	box-sizing: border-box;",
			"	-webkit-text-size-adjust: 100%;",
			"	line-height: 1.15;"
			"}",
			"",
			"*, *::before, *::after {",
			"	box-sizing: inherit;",
			"}",
			"",
			"body, h1, h2, h3, h4, h5, h6, p,
			figure, blockquote, dl, dd {",
			"	margin: 0;",
			"}",
			"",
			"body {",
			"	min-height: 100vh;",
			"	font-family: ;",
			"	 text-rendering: optimizeSpeed;",
			"}",
			"",
			"ul, ol {",
			"	list-style: none;",
			"	padding: 0;",
			"	margin: 0;",
			"}",
			""
			"a {",
			"text-decoration: none;",
			"  background-color: transparent;",
			"  color: inherit;",
			"transition: all .5s;",
			"}",
			"",
			"img, video {",
			"	max-width: 100%;"
			"	height: auto",
			"}",
			"",
			"button, input, select, textarea {",
			"	font: inherit;",
			"	margin: 0;",
			"	border-radius: 0;",
			"}",
			"",
			"button {",
			"	cursor: pointer;",
			"}",
			"",
			"textarea {",
			"	resize: vertical;",
			"}",
			"",
			"table {",
			"	border-collapse: collapse;",
			"	border-spacing: 0;",
			"}",
			"",
			"inner {",
			"width: $1;",
			"}",
			
		]
	},
	"absoluteCenter": {
		"prefix": "absolutecenter",
		"body": [
		  "position: absolute;",
		  "top: 50%;",
		  "left: 50%;",
		  "transform: translate(-50%, -50%);",
		]
	},
	"flexproperty": {
		"prefix": "flex",
		"body": [
			"display: flex;",
			"flex-direction: auto;",
			"align-item: center;",
			"justtify-content: center;",
		]
	},
	"swiperproperty": {
		"prefix": "swiper",
		"body": [
			".swiper {",
			"	",
			"}",
			"",
			".swiper-wrapper {",
			"}",
			"",
			".swiper-slide {",
			"}",
			"",
			"/* 前への矢印 */",
			".swiper-button-prev {",
			"}",
			"",
			"/* 次への矢印 */"
			".swiper-button-next {",
			"}",
			"",
			"/* ページネーションの余白 */",
			".swiper-horizontal > .swiper-pagination-bullets .swiper-pagination-bullet,",
			".swiper-pagination-horizontal.swiper-pagination-bullets .swiper-pagination-bullet {",
			"}",
			"",
			"/* ページネーションのサイズと色 */",
			".swiper-pagination-bullet {",
			"}",
		]
	}
}
